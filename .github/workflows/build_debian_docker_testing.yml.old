name: Debian unit testing

on:
  push:
    branches: [ master, 6.0 ]
  pull_request:
    branches: [ master ]

jobs:

  build:

    runs-on: [self-hosted, linux, x64, meerkat]

    steps:
    - uses: actions/checkout@v2
    - name: Building the Docker image...
      run: docker build . --file ./tools/unittesting/debian_trilinos_py3
