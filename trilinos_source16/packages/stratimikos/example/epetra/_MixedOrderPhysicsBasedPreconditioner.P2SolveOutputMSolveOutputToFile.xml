<!-- This parameters file produces output from P2 to the console and output
from M11 and M22 are sent to file. -->
<ParameterList>

  <ParameterList name="M11 Solver">
    <ParameterList name="Linear Solver Types">
      <ParameterList name="AztecOO">
        <ParameterList name="VerboseObject">
          <Parameter name="Verbosity Level" type="string" value="medium"/>
          <Parameter name="Output File" type="string" value="M11Solver.out"/>
        </ParameterList>
      </ParameterList>
    </ParameterList>
  </ParameterList>

  <ParameterList name="M22 Solver">
    <ParameterList name="Linear Solver Types">
      <ParameterList name="AztecOO">
        <ParameterList name="VerboseObject">
          <Parameter name="Verbosity Level" type="string" value="medium"/>
          <Parameter name="Output File" type="string" value="M22Solver.out"/>
        </ParameterList>
      </ParameterList>
    </ParameterList>
  </ParameterList>

  <ParameterList name="P2 Solver">
    <ParameterList name="Linear Solver Types">
      <ParameterList name="AztecOO">
        <ParameterList name="VerboseObject">
          <Parameter name="Verbosity Level" type="string" value="medium"/>
        </ParameterList>
      </ParameterList>
    </ParameterList>
  </ParameterList>

</ParameterList>
