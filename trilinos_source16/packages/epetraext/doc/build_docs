#!/bin/bash

if [ $TRILINOS_HOME ]; then
  echo "TRILINOS_HOME has already been set!"
else
  echo "TRILINOS_HOME has not been set.  Setting it!"
  export TRILINOS_HOME=`pwd`/../../..
fi

echo
echo "Generating main epetraext doxygen documentation ..."
echo

doxygen DoxyfileWeb

echo
echo "Generating doxygen browser documentation for all of epetraext ..."
echo

doxygen ../browser/doc/Doxyfile
